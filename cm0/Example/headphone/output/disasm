unsigned char IPC_TxBufferIsEnough(uint8_t Len)
{
	uint32_t TxPtr = HR_REG_24BIT(reg_map(IPC_TX_WRITE_PTR));
	uint32_t TxReadPtr = HR_REG_24BIT(reg_map(IPC_TX_READ_PTR));

	for (uint16_t i=0; i<Len; i++) {
    6876:	2312      	movs	r3, #18
    6878:	18fb      	adds	r3, r7, r3
    687a:	881a      	ldrh	r2, [r3, #0]
    687c:	2312      	movs	r3, #18
    687e:	18fb      	adds	r3, r7, r3
    6880:	3201      	adds	r2, #1
    6882:	801a      	strh	r2, [r3, #0]
    6884:	1dfb      	adds	r3, r7, #7
    6886:	781b      	ldrb	r3, [r3, #0]
    6888:	b29b      	uxth	r3, r3
    688a:	2212      	movs	r2, #18
    688c:	18ba      	adds	r2, r7, r2
    688e:	8812      	ldrh	r2, [r2, #0]
    6890:	429a      	cmp	r2, r3
    6892:	d3e0      	bcc.n	6856 <IPC_TxBufferIsEnough+0x2e>

		if(TxPtr == TxReadPtr)
			return 0;

	}
	return 1;
    6894:	2301      	movs	r3, #1
}
