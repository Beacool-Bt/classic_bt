void GPIO_SetInput(GPIO_NUM gpio)
{
	hw_delay();
	if (gpio == GPIO_MAX_NUM)
		return;
    782e:	46c0      	nop			; (mov r8, r8)
	if (gpio & GPIO_ACTIVE_BIT)
		HWRITE(CORE_GPIO_CONF +(gpio & GPIO_NUM_MASK) , GPCFG_PULLDOWN);
	else
		HWRITE(CORE_GPIO_CONF + gpio, GPCFG_PULLUP);
	hw_delay();
}
