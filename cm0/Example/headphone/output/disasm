0000771c <hw_delay>:
//#define CLRBIT(reg, val)	HWRITE(reg, HREAD(reg) & ~(val))

#define BW(addr)			(int)*(addr) << 24 | (int)*(addr + 1) << 16 | (int)*(addr + 2) << 8 | *(addr + 3)

static inline void hw_delay() 
{
    771c:	b580      	push	{r7, lr}
    771e:	af00      	add	r7, sp, #0
	__asm__ __volatile__("nop");
    7720:	46c0      	nop			; (mov r8, r8)
	__asm__ __volatile__("nop");
    7722:	46c0      	nop			; (mov r8, r8)
	__asm__ __volatile__("nop");
    7724:	46c0      	nop			; (mov r8, r8)
	__asm__ __volatile__("nop");
    7726:	46c0      	nop			; (mov r8, r8)
	__asm__ __volatile__("nop");
    7728:	46c0      	nop			; (mov r8, r8)
}
