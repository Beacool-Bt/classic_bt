void GPIO_SetOut(GPIO_NUM gpio, bool BitVal)
{
	hw_delay();
	if (gpio == GPIO_MAX_NUM)
		return;
    77cc:	46c0      	nop			; (mov r8, r8)
			HWRITE(CORE_GPIO_CONF + gpio, GPCFG_OUTPUT_LOW);
		else
			HWRITE(CORE_GPIO_CONF + gpio, GPCFG_OUTPUT_HIGH);
	}
	hw_delay();
}
