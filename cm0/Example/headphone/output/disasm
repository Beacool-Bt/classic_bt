00002e2c <tws_slave_check_process_clk_info>:
#define TWS_MEDIA_MISS_MATCH_LIMIT        (40)// unit 312.5 if clock diff big to 10ms, there may be something wrong, need request reset
uint8_t gClkMissMatch;
void tws_slave_check_process_clk_info(uint32_t clkLocal, uint32_t clkRemote) 
{
    2e2c:	b580      	push	{r7, lr}
    2e2e:	b084      	sub	sp, #16
    2e30:	af00      	add	r7, sp, #0
    2e32:	6078      	str	r0, [r7, #4]
    2e34:	6039      	str	r1, [r7, #0]
	uint32_t diffClk = 0;
    2e36:	2300      	movs	r3, #0
    2e38:	60fb      	str	r3, [r7, #12]
	if(clkRemote > clkLocal)
    2e3a:	683a      	ldr	r2, [r7, #0]
    2e3c:	687b      	ldr	r3, [r7, #4]
    2e3e:	429a      	cmp	r2, r3
    2e40:	d904      	bls.n	2e4c <tws_slave_check_process_clk_info+0x20>
	{
		diffClk = clkRemote - clkLocal;
    2e42:	683a      	ldr	r2, [r7, #0]
    2e44:	687b      	ldr	r3, [r7, #4]
    2e46:	1ad3      	subs	r3, r2, r3
    2e48:	60fb      	str	r3, [r7, #12]
	else
	{
		diffClk = clkLocal - clkRemote;
	}

	DEBUG_LOG(LOG_LEVEL_CORE, "IPC" ,"tws_slave_check_process_clk_info diffClk: 0x%04X", LOG_POINT_B015, diffClk);
    2e4a:	e003      	b.n	2e54 <tws_slave_check_process_clk_info+0x28>
	{
		diffClk = clkRemote - clkLocal;
	}
	else
	{
		diffClk = clkLocal - clkRemote;
    2e4c:	687a      	ldr	r2, [r7, #4]
    2e4e:	683b      	ldr	r3, [r7, #0]
    2e50:	1ad3      	subs	r3, r2, r3
    2e52:	60fb      	str	r3, [r7, #12]
	}

	DEBUG_LOG(LOG_LEVEL_CORE, "IPC" ,"tws_slave_check_process_clk_info diffClk: 0x%04X", LOG_POINT_B015, diffClk);
    2e54:	46c0      	nop			; (mov r8, r8)
	DEBUG_LOG(LOG_LEVEL_CORE, "IPC" ,"tws_slave_check_process_clk_info diffClk: 0x%04X", LOG_POINT_B016, gClkMissMatch);
    2e56:	46c0      	nop			; (mov r8, r8)
	
	if(diffClk > 1000)
    2e58:	68fa      	ldr	r2, [r7, #12]
    2e5a:	23fa      	movs	r3, #250	; 0xfa
    2e5c:	009b      	lsls	r3, r3, #2
    2e5e:	429a      	cmp	r2, r3
    2e60:	d81f      	bhi.n	2ea2 <tws_slave_check_process_clk_info+0x76>
	{
		// Clk overflow we didn't care
	}
	else if(diffClk > TWS_MEDIA_MISS_MATCH_LIMIT)
    2e62:	68fb      	ldr	r3, [r7, #12]
    2e64:	2b28      	cmp	r3, #40	; 0x28
    2e66:	d905      	bls.n	2e74 <tws_slave_check_process_clk_info+0x48>
	{
		gClkMissMatch = 0;
    2e68:	4b14      	ldr	r3, [pc, #80]	; (2ebc <tws_slave_check_process_clk_info+0x90>)
    2e6a:	2200      	movs	r2, #0
    2e6c:	701a      	strb	r2, [r3, #0]
		Bt_SndCmdSlaveResetAudio();
    2e6e:	f7ff ff1b 	bl	2ca8 <Bt_SndCmdSlaveResetAudio>
    2e72:	e016      	b.n	2ea2 <tws_slave_check_process_clk_info+0x76>
	}
	else if(diffClk > (TWS_MEDIA_MISS_MATCH_LIMIT/2))
    2e74:	68fb      	ldr	r3, [r7, #12]
    2e76:	2b14      	cmp	r3, #20
    2e78:	d906      	bls.n	2e88 <tws_slave_check_process_clk_info+0x5c>
	{
		gClkMissMatch += 5;
    2e7a:	4b10      	ldr	r3, [pc, #64]	; (2ebc <tws_slave_check_process_clk_info+0x90>)
    2e7c:	781b      	ldrb	r3, [r3, #0]
    2e7e:	3305      	adds	r3, #5
    2e80:	b2da      	uxtb	r2, r3
    2e82:	4b0e      	ldr	r3, [pc, #56]	; (2ebc <tws_slave_check_process_clk_info+0x90>)
    2e84:	701a      	strb	r2, [r3, #0]
    2e86:	e00c      	b.n	2ea2 <tws_slave_check_process_clk_info+0x76>
	}
	else if(diffClk > (TWS_MEDIA_MISS_MATCH_LIMIT/4))
    2e88:	68fb      	ldr	r3, [r7, #12]
    2e8a:	2b0a      	cmp	r3, #10
    2e8c:	d906      	bls.n	2e9c <tws_slave_check_process_clk_info+0x70>
	{
		gClkMissMatch += 1;
    2e8e:	4b0b      	ldr	r3, [pc, #44]	; (2ebc <tws_slave_check_process_clk_info+0x90>)
    2e90:	781b      	ldrb	r3, [r3, #0]
    2e92:	3301      	adds	r3, #1
    2e94:	b2da      	uxtb	r2, r3
    2e96:	4b09      	ldr	r3, [pc, #36]	; (2ebc <tws_slave_check_process_clk_info+0x90>)
    2e98:	701a      	strb	r2, [r3, #0]
    2e9a:	e002      	b.n	2ea2 <tws_slave_check_process_clk_info+0x76>
	}
	else
	{
		gClkMissMatch = 0;
    2e9c:	4b07      	ldr	r3, [pc, #28]	; (2ebc <tws_slave_check_process_clk_info+0x90>)
    2e9e:	2200      	movs	r2, #0
    2ea0:	701a      	strb	r2, [r3, #0]
	}

