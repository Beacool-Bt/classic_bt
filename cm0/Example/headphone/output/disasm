		if((HREAD(mem_tws_record_state)&0xf0) == 0x50){
     e98:	4b0a      	ldr	r3, [pc, #40]	; (ec4 <Bt_PowerOnKeyRelease+0x38>)
     e9a:	781b      	ldrb	r3, [r3, #0]
