00007af8 <gpioGetBit>:


bool gpioGetBit(uint8_t Num, uint32_t regBase)
{
    7af8:	b580      	push	{r7, lr}
    7afa:	b084      	sub	sp, #16
    7afc:	af00      	add	r7, sp, #0
    7afe:	0002      	movs	r2, r0
    7b00:	6039      	str	r1, [r7, #0]
    7b02:	1dfb      	adds	r3, r7, #7
    7b04:	701a      	strb	r2, [r3, #0]
	uint8_t queue = 0;
    7b06:	230f      	movs	r3, #15
    7b08:	18fb      	adds	r3, r7, r3
    7b0a:	2200      	movs	r2, #0
    7b0c:	701a      	strb	r2, [r3, #0]
	uint8_t group = 0;
    7b0e:	230e      	movs	r3, #14
    7b10:	18fb      	adds	r3, r7, r3
    7b12:	2200      	movs	r2, #0
    7b14:	701a      	strb	r2, [r3, #0]
	uint8_t st = 0;
    7b16:	230d      	movs	r3, #13
    7b18:	18fb      	adds	r3, r7, r3
    7b1a:	2200      	movs	r2, #0
    7b1c:	701a      	strb	r2, [r3, #0]
	
	queue = (Num & 7);
    7b1e:	230f      	movs	r3, #15
    7b20:	18fb      	adds	r3, r7, r3
    7b22:	1dfa      	adds	r2, r7, #7
    7b24:	7812      	ldrb	r2, [r2, #0]
    7b26:	2107      	movs	r1, #7
    7b28:	400a      	ands	r2, r1
    7b2a:	701a      	strb	r2, [r3, #0]
	group = Num >> 3 & 3;
    7b2c:	1dfb      	adds	r3, r7, #7
    7b2e:	781b      	ldrb	r3, [r3, #0]
    7b30:	08db      	lsrs	r3, r3, #3
    7b32:	b2da      	uxtb	r2, r3
    7b34:	230e      	movs	r3, #14
    7b36:	18fb      	adds	r3, r7, r3
    7b38:	2103      	movs	r1, #3
    7b3a:	400a      	ands	r2, r1
    7b3c:	701a      	strb	r2, [r3, #0]
	st = HREAD(regBase+group);
    7b3e:	230e      	movs	r3, #14
    7b40:	18fb      	adds	r3, r7, r3
    7b42:	781a      	ldrb	r2, [r3, #0]
    7b44:	683b      	ldr	r3, [r7, #0]
    7b46:	18d3      	adds	r3, r2, r3
    7b48:	001a      	movs	r2, r3
    7b4a:	2380      	movs	r3, #128	; 0x80
    7b4c:	055b      	lsls	r3, r3, #21
    7b4e:	4313      	orrs	r3, r2
    7b50:	001a      	movs	r2, r3
    7b52:	230d      	movs	r3, #13
    7b54:	18fb      	adds	r3, r7, r3
    7b56:	7812      	ldrb	r2, [r2, #0]
    7b58:	701a      	strb	r2, [r3, #0]
	return (st & (1 << queue));
    7b5a:	230d      	movs	r3, #13
