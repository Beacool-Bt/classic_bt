    67f0:	d3d2      	bcc.n	6798 <ipcTx+0x24>
		if(++TxPtr >= IPC_TX_END) 
			TxPtr = IPC_TX_HEAD;
		me=100;
		while(me--);
	}
	HWRITE24BIT(IPC_TX_WRITE_PTR, TxPtr);
    67f2:	4b09      	ldr	r3, [pc, #36]	; (6818 <ipcTx+0xa4>)
