void __attribute__((noinline,aligned(32))) QSPI_ReadFlashData(int flash_addr, int len, uint8_t *rxbuf)
{
	int i=0,j=0,x=0;

	for(i = len,j=0;i>0;i = i -0x40,j = j+0x40){
    7ed2:	69fb      	ldr	r3, [r7, #28]
    7ed4:	3b40      	subs	r3, #64	; 0x40
    7ed6:	61fb      	str	r3, [r7, #28]
    7ed8:	69bb      	ldr	r3, [r7, #24]
    7eda:	3340      	adds	r3, #64	; 0x40
    7edc:	61bb      	str	r3, [r7, #24]
    7ede:	69fb      	ldr	r3, [r7, #28]
    7ee0:	2b00      	cmp	r3, #0
    7ee2:	dcde      	bgt.n	7ea2 <QSPI_ReadFlashData+0x22>
		if(i>0x40)
			ReadFlashDSPI(FlashCMD_ReadDual,flash_addr+j,0x40,rxbuf+j);
		else
			ReadFlashDSPI(FlashCMD_ReadDual,flash_addr+j,i,rxbuf+j);	
	}
}
