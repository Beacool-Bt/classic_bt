			Bt_SndCmdStartReconTws();
     ea8:	f001 fbc6 	bl	2638 <Bt_SndCmdStartReconTws>
		else{
			VP_snd_voiceEvt(VP_POWERON);
			Bt_CheckReconnectAction();
		}
	}
}
     eac:	e004      	b.n	eb8 <Bt_PowerOnKeyRelease+0x2c>
		//Bt_CheckReconnectAction();
		if((HREAD(mem_tws_record_state)&0xf0) == 0x50){
			Bt_SndCmdStartReconTws();
