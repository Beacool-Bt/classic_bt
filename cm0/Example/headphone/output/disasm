     bea:	4293      	cmp	r3, r2
     bec:	d901      	bls.n	bf2 <SysTick_Config+0x16>
  {
    return (1UL);                                                   /* Reload value impossible */
     bee:	2301      	movs	r3, #1
     bf0:	e010      	b.n	c14 <SysTick_Config+0x38>
  }

  SysTick->LOAD  = (uint32_t)(ticks - 1UL);                         /* set reload register */
     bf2:	4b0b      	ldr	r3, [pc, #44]	; (c20 <SysTick_Config+0x44>)
     bf4:	687a      	ldr	r2, [r7, #4]
